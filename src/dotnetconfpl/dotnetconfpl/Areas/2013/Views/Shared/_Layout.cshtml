<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>dotnetconf.pl</title>
    <!--Stylesheets-->
    <link href="@Url.Content("~/Content/twitterbootstrap/css/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/twitterbootstrap/css/bootstrap-responsive.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/CustomStyle.css")" rel="stylesheet" type="text/css" />
    <!--Scripts-->
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/twitterbootstrap/js/bootstrap.js")" type="text/javascript"></script>

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" type="image/x-icon" href="/Content/images/fav.png">
    <script type="text/javascript">

        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-15894596-5']);
        _gaq.push(['_trackPageview']);

        (function () {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();
    </script>

    <link rel="canonical" href="http://dotnetconf.pl/" />

    <!-- Open Graph Tags -->
    <meta property='og:locale' content='pl_PL'/>
    <meta property='og:type' content='website'/>
    <meta property='og:title' content='dotNetConfPL - wirtualna konferencja dla .NET developerów'/>
    <meta property='og:url' content='http://dotnetconf.pl/'/>
    <meta property='og:site_name' content='dotNetConfPL'/>
    <!-- End Open Graph Tags -->

    <meta content="dotNetConfPL,.NET,C#,ASP.NET,wirtualna,konferencja,developer,Open Source,Azure,Node.js,Windows,Phone" name="keywords" />
    <meta name="description" content="Oficjalna strona wirtualnej konferencji dla developerów dotNetConfPL." />
    <meta name="author" content="Michał Franc, Jakub Jedryszek, Paweł Sawicz" />
    <link rel="author" href="https://plus.google.com/110590039840710973077?rel=author" />
    <meta name="application-name" content="dotNetConfPL" />
    <meta name="msapplication-tooltip" content="dotNetConfPL" />
</head>
<body>    
    <div class="container-fluid" style="background-color: white; padding-top: 20px">
        <div class="row-fluid">
            <div class="span12">
                <a class="brand" href="/">
                    <img src="~/Content/images/logo.png" />
                </a>
                <a title="Facebook" class="image-link"
                    href="https://www.facebook.com/dotnetconfpl"
                    target="_blank">
                    <span>
                        <img width="40" height="40" src="~/Content/images/facebook.png" alt="Facebook" />
                    </span>
                </a>

                <a title="Twitter" class="image-link"
                    href="https://twitter.com/dotNetConfPL"
                    target="_blank">
                    <span>
                        <img width="40" height="40" src="~/Content/images/twitter.png" alt="Twitter" />
                    </span>
                </a>
            </div>
        </div>
        <div class="row-fluid">
            <div class="span12">
                <div class="navbar">
                    <div class="navbar-inner">
                        <div class="container">
                            <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </a>
                            <div class="nav-collapse collapse">
                                <ul class="nav" style="font-family: 'Segoe UI', Segoe, Arial, Verdana, sans-serif; font-size: 24px; padding: 20px; text-align: center">
                                    @*<li><a href="http://dotnetconfpl.eventbrite.com/">Rejestracja</a></li>*@
                                    <li><a href="@Url.Action("Speakers")">Prelegenci</a></li>
                                    <li><a href="@Url.Action("Agenda")">Agenda (<b>VIDEO</b>)</a></li>
                                    <li><a href="@Url.Action("Contact")">Organizatorzy</a></li>
                                    <li><a href="https://jabbr.net/#/rooms/dotnetConfPL2013">JabbR</a></li>
                                </ul>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row-fluid" style="padding-top: 30px">
        @RenderBody()
    </div>
    <div class="row-fluid footer">
        <div class="span12">
            <p>Copyright © 2013 - @DateTime.Now.Year dotnetConfPL - dotnetConf.pl, logo by <a href="http://www.facebook.com/ProjectJeziorka">Project Jeziorka</a>. Wszelkie prawa zastrzeżone.</p>
        </div>
    </div>  
</body>
</html>
