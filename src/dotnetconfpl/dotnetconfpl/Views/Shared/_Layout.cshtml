<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>dotnetconf.pl</title>
    <!--Stylesheets-->
    <link href="@Url.Content("~/Content/twitterbootstrap/css/bootstrap-responsive.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/twitterbootstrap/css/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/CustomStyle.css")" rel="stylesheet" type="text/css" />
    <!--Scripts-->
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/twitterbootstrap/js/bootstrap.js")" type="text/javascript"></script>

    <script type="text/javascript">

        //Edit data below to your personal preferences ----------------------------------

        //Give the date ---------------------------------
        year = 2013; month = 10; day = 12;
        //Give the point of time ------------------------
        hour = 15; min = 0; sec = 0;
        //-----------------------------------------------

        //Standard text ---------------------------------
        main_titel = 'Rozruszajmy polski .Net';
        sub_titel = ''
        social_network_titel = '';
        //-----------------------------------------------

        //URL's --- (use 'hide' to hide) ----------------
        facebook = 'https://www.facebook.com/dotnetconf.pl.5';
        twitter = 'https://twitter.com/dotNetConfPL';
        rss = 'hide';
        deviantart = 'hide';
        myspace = 'hide';
        lastfm = 'hide';
        flikr = 'hide';
        //-----------------------------------------------

        //End editing -------------------------------------------------------------------
    </script>
    <link rel="shortcut icon" type="image/x-icon" href="/Content/countdown/img/favicon.ico">
    <link rel="stylesheet" type="text/css" href="/Content/countdown/css/style.css">
    <script type="text/javascript" src="/Content/countdown/js/jquery-1.3.2.min.js"></script>
    <script type="text/javascript" src="/Content/countdown/js/counter.js"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>
    <div class="container-fluid" style="background-color: white; width: 100%">
        <div class="row-fluid">
            <div class="span12">
                <a class="brand" href="/">
                    <img src="~/Content/images/logo.png" />
                </a>
            </div>
        </div>
        <div class="row-fluid">
            <div class="span12">
                <div class="navbar">
                    <div class="navbar-inner">
                        <div class="container">
                            <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </a>
                            <div class="nav-collapse collapse">
                                <div class="nav-bar" style="font-family: 'Segoe UI', Segoe, Arial, Verdana, sans-serif; font-size: 24px; padding: 20px; text-align: center">
                                    <a href="/Register">Rejestracja</a>
                                    <a href="/Home/Speakers">Prelegenci</a>
                                    <a href="/Home/Agenda">Agenda</a>
                                    <a href="http://jabbr.net/#/rooms/dotnetConfPL">JabbR</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
        </div>
    </div>
    <div class="row-fluid" style="padding-top: 30px">
        @RenderBody()
        <noscript>
            <div id="noscript">
                (please enable javascript)<span>Nowe wydarzenie na polskiej scenie .Netowej nachodzi</span>
            </div>
        </noscript>
    </div>
</body>
</html>
