<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>dotnetconf.pl</title>
    <!--Stylesheets-->
    <link href="@Url.Content("~/Content/twitterbootstrap/css/bootstrap-responsive.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/twitterbootstrap/css/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/CustomStyle.css")" rel="stylesheet" type="text/css" />
    <!--Scripts-->
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/twitterbootstrap/js/bootstrap.js")" type="text/javascript"></script>

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>
    <div class="container-fluid" style="background-color: white; width: 100%; padding-top: 20px">
        <div class="row-fluid">
            <div class="span12">
                <a class="brand" href="/">
                    <img src="~/Content/images/logo.png" />
                </a>
            </div>
        </div>
        <div class="row-fluid">
            <div class="span12">
                <div class="navbar">
                    <div class="navbar-inner">
                        <div class="container">
                            <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </a>
                            <div class="nav-collapse collapse">
                                <div class="nav-bar" style="font-family: 'Segoe UI', Segoe, Arial, Verdana, sans-serif; font-size: 24px; padding: 20px; text-align: center">
                                    <a href="/Register">Rejestracja</a>
                                    <a href="/Home/Speakers">Prelegenci</a>
                                    <a href="/Home/Agenda">Agenda</a>
                                    <a href="/Home/Contact">Kontakt</a>
                                    <a href="https://jabbr.net/#/rooms/dotnetConfPL2013">JabbR</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
        </div>
    </div>
    <div class="row-fluid" style="padding-top: 30px">
        <a title="Facebook" class="image-link"
            href="http://www.facebook.com/sharer.php?s=100&p[title]=DotNetConf.pl&p[summary]=Rozruszajmy polski .Net&p[url]=www.dotnetconf.pl"
            target="_blank">
            <span>
                <img width="40" height="40" src="~/Content/images/facebook.png" alt="Facebook" />
            </span>
        </a>

        <a title="Twitter" class="image-link"
            href="http://twitter.com/share?text=Wirtualna Konferencja DotNetConf.pl - Rozruszajmy Polski .Net www.dotnetconf.pl &url=www.dotnetconf.pl"
            target="_blank">
            <span>
                <img width="40" height="40" src="~/Content/images/twitter.png" alt="Twitter" />
            </span>
        </a>
        @RenderBody()
    </div>
</body>
</html>
