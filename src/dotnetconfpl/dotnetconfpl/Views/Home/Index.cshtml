@model IEnumerable<dotnetconfpl.Model.Session>

@{
    ViewBag.Title = "dotnetconf.pl";
}

<header style="background-image: url('/Content/images/dotNetConfPL-background.jpg'); background-size: cover">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="intro-text">
                    <br />
                    <span class="name">5 Listopada 2016</span>
                    <span class="skills">15:00 - 22:00</span>
                    <hr>
                    <span class="skills">
                        Wirtualna konferencja dla .NET developerów.
                    </span>
                </div>
            </div>
        </div>
    </div>
</header>

<section>
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2>Chcesz poprowadzić sesję?</h2>
                <br /><br />
                <a href="@Url.Action("CallForPapers")" class="btn btn-primary btn-lg">Call For Papers</a>
            </div>
        </div>
    </div>
</section>
<section>
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2>Chcesz się zaangażować?</h2>
                <br />
                <br />
                <a href="@Url.Action("OnSiteEvents")" class="btn btn-primary btn-lg">Więcej szczegółów!</a>
            </div>
        </div>
    </div>
</section>

<section>
    <div class="container">
        <div class="col-lg-12 text-center">
            <h2>Partnerzy</h2>
            <hr class="partners-primary">
        </div>
        <div class="col-lg-12 text-center" style="margin-top: 20px">
            <a href="http://devday.pl/" style="display:inline-block; margin-top: 50px;">
                <img src="~/Content/images/devday_logo_2015.jpg" style="pointer-events: none;" />
            </a>
            <br />
            <a href="http://devwarsztaty.pl" style="display:inline-block; margin-top: 50px;">
                <img src="~/Content/images/devWarsztaty.jpg" style="pointer-events: none;" />
            </a>
            <br />
            <a href="http://devtalk.pl" style="display: inline-block; margin-top: 50px;">
                <img src="~/Content/images/devtalk.png" style="pointer-events: none;" />
            </a>
            <br />
            <a href="http://wrocsharp.com/" style="display:inline-block; margin-top: 50px;">
                <img src="~/Content/images/wrocsharp.jpg" style="pointer-events: none;" width="250" height="67" />
            </a>
            <br />
            <a href="http://net.developerdays.pl/" style="display:inline-block; margin-top: 50px;">
                <img src="~/Content/images/developer_days.png" style="pointer-events: none;" />
            </a>
        </div>
    </div>
</section>
