@model dynamic

@{
    ViewBag.Title = "dotnetconf.pl";
}

<header>
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <img class="img-responsive" src="~/images/logo-white.png" width="350" alt="">
                <div class="intro-text">
                    <span class="name">24 października 2015</span>
                    <span class="skills">15:00 - 23:00</span>
                    <hr class="star-light">
                    <span class="skills">
                        Wirtualna konferencja dla .NET developerów.
                    </span>
                </div>
            </div>
        </div>
    </div>
</header>

<section id="cfp">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2>Call For Papers</h2>
                <hr class="star-primary">
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12 text-center">
                <p>Masz ciekawy pomysł na prezentacje ? Pracujesz nad czymś oryginalnym ? W tym roku otwieramy Call For Papers. Wkrótce więcej informacji.</p>
            </div>
        </div>
    </div>
</section>
